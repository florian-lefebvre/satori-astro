{
	"name": "satori-astro",
	"version": "0.3.3",
	"description": "Astro utilities to help you generate OpenGraph images using satori.",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepublishOnly": "pnpm build"
	},
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro"
	],
	"author": "florian-lefebvre",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/florian-lefebvre/satori-astro.git"
	},
	"bugs": "https://github.com/florian-lefebvre/satori-astro/issues",
	"homepage": "https://github.com/florian-lefebvre/satori-astro#readme",
	"license": "MIT",
	"peerDependencies": {
		"astro": "^4.12.0 || ^5.0.0-beta.1",
		"sharp": ">=0.33.3 <1.0.0"
	},
	"dependencies": {
		"satori": "^0.19.1"
	},
	"devDependencies": {
		"tsup": "^8.5.1"
	}
}
